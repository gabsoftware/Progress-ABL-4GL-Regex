DEFINE TEMP-TABLE tt_matches{1}
    FIELD NUM    AS INTEGER
    FIELD VAL    AS CHARACTER
    FIELD NBSM   AS INTEGER
    FIELD OSTART AS INTEGER
    FIELD OEND   AS INTEGER
    FIELD LEN    AS INTEGER

    INDEX PID1_NUM IS UNIQUE PRIMARY NUM
.

DEFINE TEMP-TABLE tt_submatches{1}
    FIELD NUM    AS INTEGER
    FIELD SNUM   AS INTEGER
    FIELD VAL    AS CHARACTER
    FIELD OSTART AS INTEGER
    FIELD OEND   AS INTEGER
    FIELD LEN    AS INTEGER

    INDEX PID1_NUM IS UNIQUE PRIMARY NUM SNUM
.